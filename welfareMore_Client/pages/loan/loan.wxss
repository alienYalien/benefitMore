  position: fixed;
  z-index: 99999;
}

.temp {
  z-index: 4;
  width: 100%;
  position: fixed;
  bottom:-95rpx;
  display: flex;
 
}

@keyframes slidown {
  from {
    transform: translateY(-180rpx);
  }

  to {
    transform: translateY(0rpx);
  }
}

.slidown {
  display: block;
  animation: slidown 0.3s ease-in both;
}

@keyframes slidup {
  from {
    transform: translateY(0rpx);
  }

  to {
    transform: translateY(-180rpx);
  }
}

.slidup {
  display: block;
  animation: slidup 0.3s ease-in both;
}

.disappear {
  display: none;
}

.fullbg {
  position: fixed;
  top: 0rpx;
  z-index: 1;
  width: 100%;
  height: 100%;
  background: rgb(1, 1, 1);
  transition: all 1s;
  opacity: 0;
}

.fullopacity {
  opacity: 0.7;
}

